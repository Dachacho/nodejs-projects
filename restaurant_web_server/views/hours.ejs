<h1>Our Hours</h1>
<% for(let day of days) { %> <% const hoursObj = operatingHours[day] ||
operatingHours['defaultHours'] %>
<section style="display: inline-flex; flex-direction: column; padding: 5px">
  <h2><%= day.toUpperCase() %></h2>
  <div style="color: <%= day === correctDay ? 'red' : 'black' %>;">
    <% if (hoursObj.open) {%>
    <p>Open: <%= hoursObj.open %></p>
    <p>Closed: <%= hoursObj.closed %></p>
    <% } else {%>
    <p>CLOSED</p>
    <% } %>
  </div>
</section>
<% }%>
